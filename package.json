{
  "name": "latin-greek-roots",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production node --import @swc-node/register/esm-register src/index-unetworking.ts",
    "start:bun": "NODE_ENV=production bun src/index-unetworking.ts",
    "dev": "nodemon  --exec 'node --import @swc-node/register/esm-register src/index-unetworking.ts'",
    "dev:express": "nodemon  --exec 'node --import @swc-node/register/esm-register src/index-express.ts'",
    "extract-data": "ts-node extract-wikipedia-data.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "benchmark:tinybench": "node --import @swc-node/register/esm-register src/renderers/benchmark.ts",
    "bun:benchmark:tinybench": "bun src/renderers/benchmark.ts",
    "benchmark:mitata": "node --import @swc-node/register/esm-register src/renderers/benchmark-mitata.ts",
    "bun:benchmark:mitata": "bun src/renderers/benchmark-mitata.ts"
  },
  "license": "MIT",
  "devDependencies": {
    "@swc-node/register": "^1.9.0",
    "@swc/core": "^1.4.6",
    "@swc/jest": "^0.2.36",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "mitata": "^0.1.11",
    "nodemon": "^3.1.0",
    "tinybench": "^2.6.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@sifrr/server": "^0.0.9",
    "express": "^4.18.3",
    "insights-js": "^1.2.11",
    "mikado": "^0.8.329",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.43.0"
  },
  "engines": {
    "node": ">=20"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    }
  },
  "nodemonConfig": {
    "restartable": "rs",
    "watch": [
      "src"
    ],
    "ext": "js,json,ts,tsx"
  }
}
